<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="project://database/Assets/LDtkVania/Resources/Styles/Level_style.uss?fileID=7433441132597879392&amp;guid=29cac97f9a0a8214e911f21f7fa01b2d&amp;type=3#Level_style" />
    <ui:VisualElement name="container-iid" style="flex-grow: 0; flex-direction: row; flex-shrink: 1;">
        <ui:TextField picking-mode="Ignore" label="Iid" binding-path="_iid" name="field-iid" readonly="true" class="level-field" />
        <ui:Button text="Copy" parse-escape-sequences="true" display-tooltip-when-elided="true" name="button-iid-copy" style="width: 40px;" />
    </ui:VisualElement>
    <ui:VisualElement name="container-scene" style="flex-grow: 0; flex-direction: row;">
        <uie:PropertyField name="field-scene" label="Scene" binding-path="_scene" style="flex-grow: 1;" />
        <ui:Button text="Destroy" parse-escape-sequences="true" display-tooltip-when-elided="true" name="button-destroy-scene" style="flex-shrink: 1; min-width: 50px; background-color: rgb(84, 16, 16);" />
        <ui:Button text="Create Scene" parse-escape-sequences="true" display-tooltip-when-elided="true" name="button-create-scene" tooltip="Creates an addressable scene containing the level Prefab. The scene will be loaded as additive scene instead of instantiating the GameObject" style="flex-grow: 1;" />
    </ui:VisualElement>
    <ui:Foldout text="Info" name="foldout-info" value="false" style="margin-left: 12px;">
        <uie:PropertyField name="field-asset" binding-path="_asset" label="Asset" class="level-info" />
        <uie:PropertyField name="field-ldtk-asset" binding-path="_levelFile" label="LDtk File" class="level-info" />
        <ui:VisualElement name="container-asset-path" style="flex-grow: 1; flex-direction: row; flex-shrink: 0;">
            <ui:TextField picking-mode="Ignore" label="Asset Path" name="field-asset-path" binding-path="_assetPath" class="level-field" />
            <ui:Button text="Copy" parse-escape-sequences="true" display-tooltip-when-elided="true" name="button-asset-path-copy" style="width: 40px;" />
        </ui:VisualElement>
        <ui:VisualElement name="container-addressable-key" style="flex-grow: 1; flex-direction: row; flex-shrink: 0;">
            <ui:TextField picking-mode="Ignore" label="Addressable Key" name="field-addressable-key" binding-path="_assetKey" class="level-field" />
            <ui:Button text="Copy" parse-escape-sequences="true" display-tooltip-when-elided="true" name="button-addressable-key-copy" style="width: 40px;" />
        </ui:VisualElement>
        <ui:VisualElement name="container-scene-addressable-key" style="flex-grow: 1; flex-direction: row; flex-shrink: 0;">
            <ui:TextField picking-mode="Ignore" label="Scene Addressable Key" name="field-scene-addressable-key" binding-path="_sceneAddressableKey" class="level-field" />
            <ui:Button text="Copy" parse-escape-sequences="true" display-tooltip-when-elided="true" name="button-scene-addressable-key-copy" style="width: 40px;" />
        </ui:VisualElement>
    </ui:Foldout>
</ui:UXML>
